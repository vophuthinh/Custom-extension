{
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "tabnine.experimentalAutoImports": true,
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.enabled": false,
    "editor.fontFamily": "JetBrains Mono",
    "editor.lineHeight": 30,
    "editor.letterSpacing": 0.5,
    "editor.fontLigatures": true,
    "window.zoomLevel": 0.5,
    "editor.fontSize": 15,
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "editor.wordWrap": "on",
    "auto-rename-tag.activationOnLanguage": ["html", "xml", "php", "javascript"],
    "editor.accessibilitySupport": "off",
    "files.autoSave": "afterDelay",
    "explorer.compactFolders": false,
    "auto-close-tag.enableAutoCloseTag": true,
    "npm-intellisense.packageSubfoldersIntellisense": false,
    "npm-intellisense.scanDevDependencies": true,
    "npm-intellisense.importES6": false,
    "npm-intellisense.importQuotes": "'",
    "npm-intellisense.importLinebreak": ";\r\n",
    "npm-intellisense.importDeclarationType": "const",
    "npm-intellisense.recursivePackageJsonLookup": true,
    "explorer.confirmDelete": false,
    "[json]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.host": "127.0.0.2",
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
    "terminal.integrated.shellArgs.windows": "-nologo",
    "terminal.integrated.fontWeightBold": "bold",
    "liveServer.settings.donotVerifyTags": true,
    "workbench.iconTheme": "vscode-icons",
    "open-in-browser.default": "Google Chrome",
    "git.confirmSync": false,
    "vscode_custom_css.imports": ["file:///C:/Code/extension/color.css"],
    "indentRainbow.colors": [
        "rgba(247, 37, 133,0.125)",
        "rgba(181, 23, 158,0.125)",
        "rgba(114, 9, 183,0.125)",
        "rgba(86, 11, 173,0.125)",
        "rgba(72, 12, 168,0.125)",
        "rgba(58, 12, 163,0.125)",
        "rgba(63, 55, 201,0.125)"
    ],
    //rainbow
    // "indentRainbow.colors": [
    //     "rgba(255,0,0,0.125)",
    //     "rgba(255,127,0,0.125)",
    //     "rgba(255,255,0,0.125)",
    //     "rgba(0,255,0,0.125)",
    //     "rgba(0,0,255,0.125)",
    //     "rgba(75,0,130,0.125)",
    //     "rgba(148,0,211,0.125)"
    // ],
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.printWidth": 120,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    "prettier.singleQuote": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "editor.cursorSmoothCaretAnimation": "on",
    "workbench.preferredLightColorTheme": "Evondev Dracula Author",
    "window.customMenuBarAltFocus": false,
    "workbench.colorTheme": "Evondev Dracula Darker Contrast"
}
